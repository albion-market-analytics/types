<%
 const itemNames = '\'' + items.map(item => item.UniqueName).join("' | '") + '\'';
 const _regions = world.map(region => {
     return `    "${region.Index}": "${region.UniqueName}",`;
 }).join('\n');
 %>

type City = 'Thetford' | 'Fort Sterling' | 'Lymhurst' | 'Bridgewatch' | 'Martlock' | 'Caerleon';
type ItemName = <%- itemNames %>


type RegionList = {
<%- _regions %>
};

type Region = keyof RegionList;
